'$HOME/.config/lsd/icons.yaml' -> '$HOME/dotfiles/.config/lsd/icons.yaml' # LsDeluxe icons config
'$HOME/.oh-my-zsh/custom/aliases.zsh' -> '$HOME/dotfiles/.oh-my-zsh/custom/aliases.zsh' # Aliases config for Zsh
'$HOME/.oh-my-zsh/custom/fzf.zsh' -> '$HOME/dotfiles/.oh-my-zsh/custom/fzf.zsh' # Fzf config for Zsh
